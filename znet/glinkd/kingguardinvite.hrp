class KingGuardInvite : public ProxyRpc
{
#define	RPC_BASECLASS	ProxyRpc
	#include "kingguardinvite"
#undef	RPC_BASECLASS

	bool Delivery(Manager::Session::ID proxy_sid, const OctetsStream& osArg)
	{
		KingGuardInviteArg arg;
		osArg >> arg;
		if( GLinkServer::GetInstance()->Send(arg.localsid, *this ) )
		{
			return true;
		}
		else
		{
			SetResult(KingGuardInviteRes(ERR_DELIVER_SEND));
			SendToSponsor();
			return false;
		}
	}

