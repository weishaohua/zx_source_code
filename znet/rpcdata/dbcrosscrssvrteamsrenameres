#ifndef __GNET_DBCROSSCRSSVRTEAMSRENAMERES_RPCDATA
#define __GNET_DBCROSSCRSSVRTEAMSRENAMERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBCrossCrssvrTeamsRenameRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		DBCrossCrssvrTeamsRenameRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		DBCrossCrssvrTeamsRenameRes(const DBCrossCrssvrTeamsRenameRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new DBCrossCrssvrTeamsRenameRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBCrossCrssvrTeamsRenameRes *r = dynamic_cast<const DBCrossCrssvrTeamsRenameRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		DBCrossCrssvrTeamsRenameRes& operator = (const DBCrossCrssvrTeamsRenameRes &rhs)
		{
			const DBCrossCrssvrTeamsRenameRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
