#ifndef __GNET_KINGGUARDINVITERES_RPCDATA
#define __GNET_KINGGUARDINVITERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class KingGuardInviteRes : public GNET::Rpc::Data
	{
	public:
		char retcode;

	public:
		KingGuardInviteRes (char l_retcode = 0)
			: retcode(l_retcode)
		{
		}

		KingGuardInviteRes(const KingGuardInviteRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new KingGuardInviteRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const KingGuardInviteRes *r = dynamic_cast<const KingGuardInviteRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		KingGuardInviteRes& operator = (const KingGuardInviteRes &rhs)
		{
			const KingGuardInviteRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
