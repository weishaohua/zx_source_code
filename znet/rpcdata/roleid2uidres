#ifndef __GNET_ROLEID2UIDRES_RPCDATA
#define __GNET_ROLEID2UIDRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class Roleid2UidRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		int userid;
		int src_zoneid;

	public:
		Roleid2UidRes (int l_retcode = -1,int l_userid = 0,int l_src_zoneid = 0)
			: retcode(l_retcode),userid(l_userid),src_zoneid(l_src_zoneid)
		{
		}

		Roleid2UidRes(const Roleid2UidRes &rhs)
			: retcode(rhs.retcode),userid(rhs.userid),src_zoneid(rhs.src_zoneid) { }

		Rpc::Data *Clone() const { return new Roleid2UidRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const Roleid2UidRes *r = dynamic_cast<const Roleid2UidRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				userid = r->userid;
				src_zoneid = r->src_zoneid;
			}
			return *this;
		}

		Roleid2UidRes& operator = (const Roleid2UidRes &rhs)
		{
			const Roleid2UidRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				userid = r->userid;
				src_zoneid = r->src_zoneid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << userid;
			os << src_zoneid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> userid;
			os >> src_zoneid;
			return os;
		}

	};
};
#endif
