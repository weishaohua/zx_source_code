#ifndef __GNET_DBSNSUPDATEROLEINFORES_RPCDATA
#define __GNET_DBSNSUPDATEROLEINFORES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBSNSUpdateRoleInfoRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		DBSNSUpdateRoleInfoRes ()
		{
		}

		DBSNSUpdateRoleInfoRes(const DBSNSUpdateRoleInfoRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new DBSNSUpdateRoleInfoRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBSNSUpdateRoleInfoRes *r = dynamic_cast<const DBSNSUpdateRoleInfoRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		DBSNSUpdateRoleInfoRes& operator = (const DBSNSUpdateRoleInfoRes &rhs)
		{
			const DBSNSUpdateRoleInfoRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
