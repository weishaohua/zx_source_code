#ifndef __GNET_DBSNSGETPLAYERINFOCACHEARG_RPCDATA
#define __GNET_DBSNSGETPLAYERINFOCACHEARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBSNSGetPlayerInfoCacheArg : public GNET::Rpc::Data
	{
	public:
		int roleid;

	public:
		DBSNSGetPlayerInfoCacheArg (int l_roleid = 0)
			: roleid(l_roleid)
		{
		}

		DBSNSGetPlayerInfoCacheArg(const DBSNSGetPlayerInfoCacheArg &rhs)
			: roleid(rhs.roleid) { }

		Rpc::Data *Clone() const { return new DBSNSGetPlayerInfoCacheArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBSNSGetPlayerInfoCacheArg *r = dynamic_cast<const DBSNSGetPlayerInfoCacheArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		DBSNSGetPlayerInfoCacheArg& operator = (const DBSNSGetPlayerInfoCacheArg &rhs)
		{
			const DBSNSGetPlayerInfoCacheArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

	};
};
#endif
