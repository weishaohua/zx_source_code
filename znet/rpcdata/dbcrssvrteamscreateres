#ifndef __GNET_DBCRSSVRTEAMSCREATERES_RPCDATA
#define __GNET_DBCRSSVRTEAMSCREATERES_RPCDATA

#include "rpcdefs.h"

#include "gmailsyncdata"
#include "gcrssvrteamsdata"

namespace GNET
{
	class DBCrssvrTeamsCreateRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		GMailSyncData syncdata;
		GCrssvrTeamsData info;

	public:
		DBCrssvrTeamsCreateRes (int l_retcode = -1,const GMailSyncData& l_syncdata = GMailSyncData())
			: retcode(l_retcode),syncdata(l_syncdata)
		{
		}

		DBCrssvrTeamsCreateRes(const DBCrssvrTeamsCreateRes &rhs)
			: retcode(rhs.retcode),syncdata(rhs.syncdata),info(rhs.info) { }

		Rpc::Data *Clone() const { return new DBCrssvrTeamsCreateRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBCrssvrTeamsCreateRes *r = dynamic_cast<const DBCrssvrTeamsCreateRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				syncdata = r->syncdata;
				info = r->info;
			}
			return *this;
		}

		DBCrssvrTeamsCreateRes& operator = (const DBCrssvrTeamsCreateRes &rhs)
		{
			const DBCrssvrTeamsCreateRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				syncdata = r->syncdata;
				info = r->info;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << syncdata;
			os << info;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> syncdata;
			os >> info;
			return os;
		}

	};
};
#endif
