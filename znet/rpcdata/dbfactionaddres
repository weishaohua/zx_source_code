#ifndef __GNET_DBFACTIONADDRES_RPCDATA
#define __GNET_DBFACTIONADDRES_RPCDATA

#include "rpcdefs.h"

#include "gmailsyncdata"
#include "gfactioninfo"

namespace GNET
{
	class DBFactionAddRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		GMailSyncData syncdata;
		GFactionInfo info;

	public:
		DBFactionAddRes (int l_retcode = -1,const GMailSyncData& l_syncdata = GMailSyncData())
			: retcode(l_retcode),syncdata(l_syncdata)
		{
		}

		DBFactionAddRes(const DBFactionAddRes &rhs)
			: retcode(rhs.retcode),syncdata(rhs.syncdata),info(rhs.info) { }

		Rpc::Data *Clone() const { return new DBFactionAddRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBFactionAddRes *r = dynamic_cast<const DBFactionAddRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				syncdata = r->syncdata;
				info = r->info;
			}
			return *this;
		}

		DBFactionAddRes& operator = (const DBFactionAddRes &rhs)
		{
			const DBFactionAddRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				syncdata = r->syncdata;
				info = r->info;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << syncdata;
			os << info;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> syncdata;
			os >> info;
			return os;
		}

	};
};
#endif
