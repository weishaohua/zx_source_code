#ifndef __GNET_POSTCRSSVRTEAMSRENAMEARG_RPCDATA
#define __GNET_POSTCRSSVRTEAMSRENAMEARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PostCrssvrTeamsRenameArg : public GNET::Rpc::Data
	{
	public:
		char success;
		int zoneid;
		int teamid;
		Octets oldname;
		Octets newname;

	public:
		PostCrssvrTeamsRenameArg (char l_success = 1,int l_zoneid = -1,int l_teamid = 0
			,const Octets& l_oldname = Octets(0),const Octets& l_newname = Octets(0))
			: success(l_success),zoneid(l_zoneid),teamid(l_teamid)
			,oldname(l_oldname),newname(l_newname)
		{
		}

		PostCrssvrTeamsRenameArg(const PostCrssvrTeamsRenameArg &rhs)
			: success(rhs.success),zoneid(rhs.zoneid),teamid(rhs.teamid),
			oldname(rhs.oldname),newname(rhs.newname) { }

		Rpc::Data *Clone() const { return new PostCrssvrTeamsRenameArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PostCrssvrTeamsRenameArg *r = dynamic_cast<const PostCrssvrTeamsRenameArg *>(&rhs);
			if (r && r != this)
			{
				success = r->success;
				zoneid = r->zoneid;
				teamid = r->teamid;
				oldname = r->oldname;
				newname = r->newname;
			}
			return *this;
		}

		PostCrssvrTeamsRenameArg& operator = (const PostCrssvrTeamsRenameArg &rhs)
		{
			const PostCrssvrTeamsRenameArg *r = &rhs;
			if (r && r != this)
			{
				success = r->success;
				zoneid = r->zoneid;
				teamid = r->teamid;
				oldname = r->oldname;
				newname = r->newname;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << success;
			os << zoneid;
			os << teamid;
			os << oldname;
			os << newname;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> success;
			os >> zoneid;
			os >> teamid;
			os >> oldname;
			os >> newname;
			return os;
		}

	};
};
#endif
