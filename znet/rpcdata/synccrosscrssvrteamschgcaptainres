			: retcode(rhs.retcode),teamid(rhs.teamid) { }

		Rpc::Data *Clone() const { return new SyncCrossCrssvrTeamsChgCaptainRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const SyncCrossCrssvrTeamsChgCaptainRes *r = dynamic_cast<const SyncCrossCrssvrTeamsChgCaptainRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				teamid = r->teamid;
			}
			return *this;
		}

		SyncCrossCrssvrTeamsChgCaptainRes& operator = (const SyncCrossCrssvrTeamsChgCaptainRes &rhs)
		{
			const SyncCrossCrssvrTeamsChgCaptainRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				teamid = r->teamid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << teamid;
			return os;
		}

