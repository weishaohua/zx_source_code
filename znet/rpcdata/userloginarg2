#ifndef __GNET_USERLOGINARG2_RPCDATA
#define __GNET_USERLOGINARG2_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class UserLoginArg2 : public GNET::Rpc::Data
	{
	public:
		int userid;
		unsigned int localsid;
		char blkickuser;
		int freecreatime;
		Octets used_elec_number;
		int reserved1;
		Octets reserved2;
		int loginip;
		Octets iseckey;
		Octets oseckey;
		Octets accountname;

	public:
		UserLoginArg2 (int l_userid = -1,unsigned int l_localsid = 0,char l_blkickuser = 0
			,int l_freecreatime = 0,const Octets& l_used_elec_number = Octets(),int l_reserved1 = 0
			,const Octets& l_reserved2 = Octets(),int l_loginip = 0,const Octets& l_iseckey = Octets()
			,const Octets& l_oseckey = Octets(),const Octets& l_accountname = Octets())
			: userid(l_userid),localsid(l_localsid),blkickuser(l_blkickuser)
			,freecreatime(l_freecreatime),used_elec_number(l_used_elec_number),reserved1(l_reserved1)
			,reserved2(l_reserved2),loginip(l_loginip),iseckey(l_iseckey)
			,oseckey(l_oseckey),accountname(l_accountname)
		{
		}

		UserLoginArg2(const UserLoginArg2 &rhs)
			: userid(rhs.userid),localsid(rhs.localsid),blkickuser(rhs.blkickuser),
			freecreatime(rhs.freecreatime),used_elec_number(rhs.used_elec_number),reserved1(rhs.reserved1),
			reserved2(rhs.reserved2),loginip(rhs.loginip),iseckey(rhs.iseckey),
			oseckey(rhs.oseckey),accountname(rhs.accountname) { }

		Rpc::Data *Clone() const { return new UserLoginArg2(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const UserLoginArg2 *r = dynamic_cast<const UserLoginArg2 *>(&rhs);
			if (r && r != this)
			{
				userid = r->userid;
				localsid = r->localsid;
				blkickuser = r->blkickuser;
				freecreatime = r->freecreatime;
				used_elec_number = r->used_elec_number;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				loginip = r->loginip;
				iseckey = r->iseckey;
				oseckey = r->oseckey;
				accountname = r->accountname;
			}
			return *this;
		}

		UserLoginArg2& operator = (const UserLoginArg2 &rhs)
		{
			const UserLoginArg2 *r = &rhs;
			if (r && r != this)
			{
				userid = r->userid;
				localsid = r->localsid;
				blkickuser = r->blkickuser;
				freecreatime = r->freecreatime;
				used_elec_number = r->used_elec_number;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				loginip = r->loginip;
				iseckey = r->iseckey;
				oseckey = r->oseckey;
				accountname = r->accountname;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << userid;
			os << localsid;
			os << blkickuser;
			os << freecreatime;
			os << used_elec_number;
			os << reserved1;
			os << reserved2;
			os << loginip;
			os << iseckey;
			os << oseckey;
			os << accountname;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> userid;
			os >> localsid;
			os >> blkickuser;
			os >> freecreatime;
			os >> used_elec_number;
			os >> reserved1;
			os >> reserved2;
			os >> loginip;
			os >> iseckey;
			os >> oseckey;
			os >> accountname;
			return os;
		}

	};
};
#endif
