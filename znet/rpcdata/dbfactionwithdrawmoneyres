#ifndef __GNET_DBFACTIONWITHDRAWMONEYRES_RPCDATA
#define __GNET_DBFACTIONWITHDRAWMONEYRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBFactionWithDrawMoneyRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		unsigned int new_money;

	public:
		DBFactionWithDrawMoneyRes (int l_retcode = -1,unsigned int l_new_money = 0)
			: retcode(l_retcode),new_money(l_new_money)
		{
		}

		DBFactionWithDrawMoneyRes(const DBFactionWithDrawMoneyRes &rhs)
			: retcode(rhs.retcode),new_money(rhs.new_money) { }

		Rpc::Data *Clone() const { return new DBFactionWithDrawMoneyRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBFactionWithDrawMoneyRes *r = dynamic_cast<const DBFactionWithDrawMoneyRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				new_money = r->new_money;
			}
			return *this;
		}

		DBFactionWithDrawMoneyRes& operator = (const DBFactionWithDrawMoneyRes &rhs)
		{
			const DBFactionWithDrawMoneyRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				new_money = r->new_money;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << new_money;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> new_money;
			return os;
		}

	};
};
#endif
