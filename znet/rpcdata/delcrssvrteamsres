#ifndef __GNET_DELCRSSVRTEAMSRES_RPCDATA
#define __GNET_DELCRSSVRTEAMSRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DelCrssvrTeamsRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		Octets teamname;
		int createtime;

	public:
		DelCrssvrTeamsRes (int l_retcode = -1,const Octets& l_teamname = Octets(0),int l_createtime = 0)
			: retcode(l_retcode),teamname(l_teamname),createtime(l_createtime)
		{
		}

		DelCrssvrTeamsRes(const DelCrssvrTeamsRes &rhs)
			: retcode(rhs.retcode),teamname(rhs.teamname),createtime(rhs.createtime) { }

		Rpc::Data *Clone() const { return new DelCrssvrTeamsRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DelCrssvrTeamsRes *r = dynamic_cast<const DelCrssvrTeamsRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				teamname = r->teamname;
				createtime = r->createtime;
			}
			return *this;
		}

		DelCrssvrTeamsRes& operator = (const DelCrssvrTeamsRes &rhs)
		{
			const DelCrssvrTeamsRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				teamname = r->teamname;
				createtime = r->createtime;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << teamname;
			os << createtime;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> teamname;
			os >> createtime;
			return os;
		}

	};
};
#endif
