#ifndef __GNET_DBGETTOUCHORDERARG_RPCDATA
#define __GNET_DBGETTOUCHORDERARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBGetTouchOrderArg : public GNET::Rpc::Data
	{
	public:
		int roleid;

	public:
		DBGetTouchOrderArg (int l_roleid = 0)
			: roleid(l_roleid)
		{
		}

		DBGetTouchOrderArg(const DBGetTouchOrderArg &rhs)
			: roleid(rhs.roleid) { }

		Rpc::Data *Clone() const { return new DBGetTouchOrderArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBGetTouchOrderArg *r = dynamic_cast<const DBGetTouchOrderArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		DBGetTouchOrderArg& operator = (const DBGetTouchOrderArg &rhs)
		{
			const DBGetTouchOrderArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

	};
};
#endif
