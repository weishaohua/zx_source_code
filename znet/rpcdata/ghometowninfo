#ifndef __GNET_GHOMETOWNINFO_RPCDATA
#define __GNET_GHOMETOWNINFO_RPCDATA

#include "rpcdefs.h"

#include "ghometownplayer"

namespace GNET
{
	class GHometownInfo : public GNET::Rpc::Data
	{
	public:
		GHometownPlayer player;
		int reserved1;
		int reserved2;
		int reserved3;
		int reserved4;
		int reserved5;
		int reserved6;
		int reserved7;
		int reserved8;

	public:
		GHometownInfo (const GHometownPlayer& l_player = GHometownPlayer(),int l_reserved1 = 0,int l_reserved2 = 0
			,int l_reserved3 = 0,int l_reserved4 = 0,int l_reserved5 = 0
			,int l_reserved6 = 0,int l_reserved7 = 0,int l_reserved8 = 0)
			: player(l_player),reserved1(l_reserved1),reserved2(l_reserved2)
			,reserved3(l_reserved3),reserved4(l_reserved4),reserved5(l_reserved5)
			,reserved6(l_reserved6),reserved7(l_reserved7),reserved8(l_reserved8)
		{
		}

		GHometownInfo(const GHometownInfo &rhs)
			: player(rhs.player),reserved1(rhs.reserved1),reserved2(rhs.reserved2),
			reserved3(rhs.reserved3),reserved4(rhs.reserved4),reserved5(rhs.reserved5),
			reserved6(rhs.reserved6),reserved7(rhs.reserved7),reserved8(rhs.reserved8) { }

		Rpc::Data *Clone() const { return new GHometownInfo(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const GHometownInfo *r = dynamic_cast<const GHometownInfo *>(&rhs);
			if (r && r != this)
			{
				player = r->player;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
				reserved5 = r->reserved5;
				reserved6 = r->reserved6;
				reserved7 = r->reserved7;
				reserved8 = r->reserved8;
			}
			return *this;
		}

		GHometownInfo& operator = (const GHometownInfo &rhs)
		{
			const GHometownInfo *r = &rhs;
			if (r && r != this)
			{
				player = r->player;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
				reserved5 = r->reserved5;
				reserved6 = r->reserved6;
				reserved7 = r->reserved7;
				reserved8 = r->reserved8;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << player;
			os << reserved1;
			os << reserved2;
			os << reserved3;
			os << reserved4;
			os << reserved5;
			os << reserved6;
			os << reserved7;
			os << reserved8;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> player;
			os >> reserved1;
			os >> reserved2;
			os >> reserved3;
			os >> reserved4;
			os >> reserved5;
			os >> reserved6;
			os >> reserved7;
			os >> reserved8;
			return os;
		}

	};
};
#endif
