#ifndef __GNET_POSTCRSSVRTEAMSCREATEARG_RPCDATA
#define __GNET_POSTCRSSVRTEAMSCREATEARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PostCrssvrTeamsCreateArg : public GNET::Rpc::Data
	{
	public:
		char success;
		int zoneid;
		int teamid;
		Octets teamname;

	public:
		PostCrssvrTeamsCreateArg (char l_success = 1,int l_zoneid = -1,int l_teamid = -1
			,const Octets& l_teamname = Octets(0))
			: success(l_success),zoneid(l_zoneid),teamid(l_teamid)
			,teamname(l_teamname)
		{
		}

		PostCrssvrTeamsCreateArg(const PostCrssvrTeamsCreateArg &rhs)
			: success(rhs.success),zoneid(rhs.zoneid),teamid(rhs.teamid),
			teamname(rhs.teamname) { }

		Rpc::Data *Clone() const { return new PostCrssvrTeamsCreateArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PostCrssvrTeamsCreateArg *r = dynamic_cast<const PostCrssvrTeamsCreateArg *>(&rhs);
			if (r && r != this)
			{
				success = r->success;
				zoneid = r->zoneid;
				teamid = r->teamid;
				teamname = r->teamname;
			}
			return *this;
		}

		PostCrssvrTeamsCreateArg& operator = (const PostCrssvrTeamsCreateArg &rhs)
		{
			const PostCrssvrTeamsCreateArg *r = &rhs;
			if (r && r != this)
			{
				success = r->success;
				zoneid = r->zoneid;
				teamid = r->teamid;
				teamname = r->teamname;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << success;
			os << zoneid;
			os << teamid;
			os << teamname;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> success;
			os >> zoneid;
			os >> teamid;
			os >> teamname;
			return os;
		}

	};
};
#endif
