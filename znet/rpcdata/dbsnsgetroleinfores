#ifndef __GNET_DBSNSGETROLEINFORES_RPCDATA
#define __GNET_DBSNSGETROLEINFORES_RPCDATA

#include "rpcdefs.h"

#include "gsnsroleinfo"

namespace GNET
{
	class DBSNSGetRoleInfoRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		int roleid;
		GSNSRoleInfo info;

	public:
		DBSNSGetRoleInfoRes (int l_roleid = 0,const GSNSRoleInfo& l_info = GSNSRoleInfo())
			: roleid(l_roleid),info(l_info)
		{
		}

		DBSNSGetRoleInfoRes(const DBSNSGetRoleInfoRes &rhs)
			: retcode(rhs.retcode),roleid(rhs.roleid),info(rhs.info) { }

		Rpc::Data *Clone() const { return new DBSNSGetRoleInfoRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBSNSGetRoleInfoRes *r = dynamic_cast<const DBSNSGetRoleInfoRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				roleid = r->roleid;
				info = r->info;
			}
			return *this;
		}

		DBSNSGetRoleInfoRes& operator = (const DBSNSGetRoleInfoRes &rhs)
		{
			const DBSNSGetRoleInfoRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				roleid = r->roleid;
				info = r->info;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << info;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> info;
			return os;
		}

	};
};
#endif
