#ifndef __GNET_POSTDELETEFACTIONARG_RPCDATA
#define __GNET_POSTDELETEFACTIONARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PostDeleteFactionArg : public GNET::Rpc::Data
	{
	public:
		int zoneid;
		int factionid;
		std::vector<Octets> namelist;

	public:
		PostDeleteFactionArg (int l_zoneid = -1,int l_factionid = -1,const std::vector<Octets>& l_namelist = std::vector<Octets>())
			: zoneid(l_zoneid),factionid(l_factionid),namelist(l_namelist)
		{
		}

		PostDeleteFactionArg(const PostDeleteFactionArg &rhs)
			: zoneid(rhs.zoneid),factionid(rhs.factionid),namelist(rhs.namelist) { }

		Rpc::Data *Clone() const { return new PostDeleteFactionArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PostDeleteFactionArg *r = dynamic_cast<const PostDeleteFactionArg *>(&rhs);
			if (r && r != this)
			{
				zoneid = r->zoneid;
				factionid = r->factionid;
				namelist = r->namelist;
			}
			return *this;
		}

		PostDeleteFactionArg& operator = (const PostDeleteFactionArg &rhs)
		{
			const PostDeleteFactionArg *r = &rhs;
			if (r && r != this)
			{
				zoneid = r->zoneid;
				factionid = r->factionid;
				namelist = r->namelist;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << zoneid;
			os << factionid;
			os << namelist;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> zoneid;
			os >> factionid;
			os >> namelist;
			return os;
		}

	};
};
#endif
