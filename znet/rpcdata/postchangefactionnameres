#ifndef __GNET_POSTCHANGEFACTIONNAMERES_RPCDATA
#define __GNET_POSTCHANGEFACTIONNAMERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PostChangeFactionnameRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		PostChangeFactionnameRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		PostChangeFactionnameRes(const PostChangeFactionnameRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new PostChangeFactionnameRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PostChangeFactionnameRes *r = dynamic_cast<const PostChangeFactionnameRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		PostChangeFactionnameRes& operator = (const PostChangeFactionnameRes &rhs)
		{
			const PostChangeFactionnameRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
