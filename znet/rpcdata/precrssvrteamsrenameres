#ifndef __GNET_PRECRSSVRTEAMSRENAMERES_RPCDATA
#define __GNET_PRECRSSVRTEAMSRENAMERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PreCrssvrTeamsRenameRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		PreCrssvrTeamsRenameRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		PreCrssvrTeamsRenameRes(const PreCrssvrTeamsRenameRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new PreCrssvrTeamsRenameRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PreCrssvrTeamsRenameRes *r = dynamic_cast<const PreCrssvrTeamsRenameRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		PreCrssvrTeamsRenameRes& operator = (const PreCrssvrTeamsRenameRes &rhs)
		{
			const PreCrssvrTeamsRenameRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
