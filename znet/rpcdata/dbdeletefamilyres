#ifndef __GNET_DBDELETEFAMILYRES_RPCDATA
#define __GNET_DBDELETEFAMILYRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBDeleteFamilyRes : public GNET::Rpc::Data
	{
	public:
		char retcode;
		std::vector<Octets> namelist;

	public:
		DBDeleteFamilyRes (char l_retcode = -1,const std::vector<Octets>& l_namelist = std::vector<Octets>())
			: retcode(l_retcode),namelist(l_namelist)
		{
		}

		DBDeleteFamilyRes(const DBDeleteFamilyRes &rhs)
			: retcode(rhs.retcode),namelist(rhs.namelist) { }

		Rpc::Data *Clone() const { return new DBDeleteFamilyRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBDeleteFamilyRes *r = dynamic_cast<const DBDeleteFamilyRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				namelist = r->namelist;
			}
			return *this;
		}

		DBDeleteFamilyRes& operator = (const DBDeleteFamilyRes &rhs)
		{
			const DBDeleteFamilyRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				namelist = r->namelist;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << namelist;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> namelist;
			return os;
		}

	};
};
#endif
