			const DBCrssvrTeamsSeasonTopSaveRes *r = dynamic_cast<const DBCrssvrTeamsSeasonTopSaveRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

