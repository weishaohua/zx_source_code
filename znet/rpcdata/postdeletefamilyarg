#ifndef __GNET_POSTDELETEFAMILYARG_RPCDATA
#define __GNET_POSTDELETEFAMILYARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PostDeleteFamilyArg : public GNET::Rpc::Data
	{
	public:
		int zoneid;
		int familyid;
		std::vector<Octets> namelist;

	public:
		PostDeleteFamilyArg (int l_zoneid = -1,int l_familyid = -1,const std::vector<Octets>& l_namelist = std::vector<Octets>())
			: zoneid(l_zoneid),familyid(l_familyid),namelist(l_namelist)
		{
		}

		PostDeleteFamilyArg(const PostDeleteFamilyArg &rhs)
			: zoneid(rhs.zoneid),familyid(rhs.familyid),namelist(rhs.namelist) { }

		Rpc::Data *Clone() const { return new PostDeleteFamilyArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PostDeleteFamilyArg *r = dynamic_cast<const PostDeleteFamilyArg *>(&rhs);
			if (r && r != this)
			{
				zoneid = r->zoneid;
				familyid = r->familyid;
				namelist = r->namelist;
			}
			return *this;
		}

		PostDeleteFamilyArg& operator = (const PostDeleteFamilyArg &rhs)
		{
			const PostDeleteFamilyArg *r = &rhs;
			if (r && r != this)
			{
				zoneid = r->zoneid;
				familyid = r->familyid;
				namelist = r->namelist;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << zoneid;
			os << familyid;
			os << namelist;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> zoneid;
			os >> familyid;
			os >> namelist;
			return os;
		}

	};
};
#endif
