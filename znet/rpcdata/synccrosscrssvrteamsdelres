#ifndef __GNET_SYNCCROSSCRSSVRTEAMSDELRES_RPCDATA
#define __GNET_SYNCCROSSCRSSVRTEAMSDELRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class SyncCrossCrssvrTeamsDelRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		int teamid;

	public:
		SyncCrossCrssvrTeamsDelRes ()
		{
		}

		SyncCrossCrssvrTeamsDelRes(const SyncCrossCrssvrTeamsDelRes &rhs)
			: retcode(rhs.retcode),teamid(rhs.teamid) { }

		Rpc::Data *Clone() const { return new SyncCrossCrssvrTeamsDelRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const SyncCrossCrssvrTeamsDelRes *r = dynamic_cast<const SyncCrossCrssvrTeamsDelRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				teamid = r->teamid;
			}
			return *this;
		}

		SyncCrossCrssvrTeamsDelRes& operator = (const SyncCrossCrssvrTeamsDelRes &rhs)
		{
			const SyncCrossCrssvrTeamsDelRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				teamid = r->teamid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << teamid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> teamid;
			return os;
		}

	};
};
#endif
