#ifndef __GNET_DBTERRITORYLISTSAVERES_RPCDATA
#define __GNET_DBTERRITORYLISTSAVERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class DBTerritoryListSaveRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		DBTerritoryListSaveRes ()
		{
		}

		DBTerritoryListSaveRes(const DBTerritoryListSaveRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new DBTerritoryListSaveRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBTerritoryListSaveRes *r = dynamic_cast<const DBTerritoryListSaveRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		DBTerritoryListSaveRes& operator = (const DBTerritoryListSaveRes &rhs)
		{
			const DBTerritoryListSaveRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
