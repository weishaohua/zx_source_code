#ifndef __GNET_DBGETTOPFLOWERDATARES_RPCDATA
#define __GNET_DBGETTOPFLOWERDATARES_RPCDATA

#include "rpcdefs.h"

#include "gtopflowerdata"

namespace GNET
{
	class DBGetTopFlowerDataRes : public GNET::Rpc::Data
	{
	public:
		std::vector<GTopFlowerData> data;
		int retcode;

	public:
		DBGetTopFlowerDataRes ()
		{
		}

		DBGetTopFlowerDataRes(const DBGetTopFlowerDataRes &rhs)
			: data(rhs.data),retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new DBGetTopFlowerDataRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const DBGetTopFlowerDataRes *r = dynamic_cast<const DBGetTopFlowerDataRes *>(&rhs);
			if (r && r != this)
			{
				data = r->data;
				retcode = r->retcode;
			}
			return *this;
		}

		DBGetTopFlowerDataRes& operator = (const DBGetTopFlowerDataRes &rhs)
		{
			const DBGetTopFlowerDataRes *r = &rhs;
			if (r && r != this)
			{
				data = r->data;
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << data;
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> data;
			os >> retcode;
			return os;
		}

	};
};
#endif
