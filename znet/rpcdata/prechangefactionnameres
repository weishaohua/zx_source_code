#ifndef __GNET_PRECHANGEFACTIONNAMERES_RPCDATA
#define __GNET_PRECHANGEFACTIONNAMERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class PreChangeFactionnameRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		PreChangeFactionnameRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		PreChangeFactionnameRes(const PreChangeFactionnameRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new PreChangeFactionnameRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const PreChangeFactionnameRes *r = dynamic_cast<const PreChangeFactionnameRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		PreChangeFactionnameRes& operator = (const PreChangeFactionnameRes &rhs)
		{
			const PreChangeFactionnameRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
