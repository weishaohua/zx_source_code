#ifndef __GNET_GETCASHAVAILARG_RPCDATA
#define __GNET_GETCASHAVAILARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class GetCashAvailArg : public GNET::Rpc::Data
	{
	public:
		int roleid;
		int userid;
		int flag;

	public:
		GetCashAvailArg (int l_roleid = 0,int l_userid = 0,int l_flag = 0)
			: roleid(l_roleid),userid(l_userid),flag(l_flag)
		{
		}

		GetCashAvailArg(const GetCashAvailArg &rhs)
			: roleid(rhs.roleid),userid(rhs.userid),flag(rhs.flag) { }

		Rpc::Data *Clone() const { return new GetCashAvailArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const GetCashAvailArg *r = dynamic_cast<const GetCashAvailArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
				userid = r->userid;
				flag = r->flag;
			}
			return *this;
		}

		GetCashAvailArg& operator = (const GetCashAvailArg &rhs)
		{
			const GetCashAvailArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
				userid = r->userid;
				flag = r->flag;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << userid;
			os << flag;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> userid;
			os >> flag;
			return os;
		}

	};
};
#endif
