	public:
		int retcode;
		int battle_id;
		int world_tag;
		enum { PROTOCOL_TYPE = PROTOCOL_SENDSIEGESTART_RE };
	public:
		SendSiegeStart_Re() { type = PROTOCOL_SENDSIEGESTART_RE; }
		SendSiegeStart_Re(void*) : Protocol(PROTOCOL_SENDSIEGESTART_RE) { }
		SendSiegeStart_Re (int l_retcode,int l_battle_id,int l_world_tag = 0)
			 : retcode(l_retcode),battle_id(l_battle_id),world_tag(l_world_tag)
		{
			type = PROTOCOL_SENDSIEGESTART_RE;
		}

		SendSiegeStart_Re(const SendSiegeStart_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),battle_id(rhs.battle_id),world_tag(rhs.world_tag) { }

		GNET::Protocol *Clone() const { return new SendSiegeStart_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << battle_id;
			os << world_tag;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> battle_id;
			os >> world_tag;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
