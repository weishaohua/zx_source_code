	public:
		int dummy;
		enum { PROTOCOL_TYPE = PROTOCOL_QUEENCLOSEBATH };
	public:
		QueenCloseBath() { type = PROTOCOL_QUEENCLOSEBATH; }
		QueenCloseBath(void*) : Protocol(PROTOCOL_QUEENCLOSEBATH) { }
		QueenCloseBath (int l_dummy)
			 : dummy(l_dummy)
		{
			type = PROTOCOL_QUEENCLOSEBATH;
		}

		QueenCloseBath(const QueenCloseBath &rhs)
			: Protocol(rhs),dummy(rhs.dummy) { }

		GNET::Protocol *Clone() const { return new QueenCloseBath(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << dummy;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> dummy;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
