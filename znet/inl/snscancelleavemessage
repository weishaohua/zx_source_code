	public:
		int roleid;
		int message_id;
		char message_type;
		short leavemsg_index;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_SNSCANCELLEAVEMESSAGE };
	public:
		SNSCancelLeaveMessage() { type = PROTOCOL_SNSCANCELLEAVEMESSAGE; }
		SNSCancelLeaveMessage(void*) : Protocol(PROTOCOL_SNSCANCELLEAVEMESSAGE) { }
		SNSCancelLeaveMessage (int l_roleid,int l_message_id = 0,char l_message_type = 0,
			short l_leavemsg_index = 0,int l_localsid = 0)
			 : roleid(l_roleid),message_id(l_message_id),message_type(l_message_type)
			,leavemsg_index(l_leavemsg_index),localsid(l_localsid)
		{
			type = PROTOCOL_SNSCANCELLEAVEMESSAGE;
		}

		SNSCancelLeaveMessage(const SNSCancelLeaveMessage &rhs)
			: Protocol(rhs),roleid(rhs.roleid),message_id(rhs.message_id),message_type(rhs.message_type)
			,leavemsg_index(rhs.leavemsg_index),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new SNSCancelLeaveMessage(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << message_id;
			os << message_type;
			os << leavemsg_index;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> message_id;
			os >> message_type;
			os >> leavemsg_index;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
