		GNET::Protocol *Clone() const {  return new DBGetTopFlowerData(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBGETTOPFLOWERDATA };
		DBGetTopFlowerData(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBGetTopFlowerData(const DBGetTopFlowerData &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 524288; }
		bool TimePolicy(int timeout) const { return timeout <= 90; }
