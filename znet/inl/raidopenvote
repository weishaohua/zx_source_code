	public:
		int roleid;
		int voted_roleid;
		int raidroom_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_RAIDOPENVOTE };
	public:
		RaidOpenVote() { type = PROTOCOL_RAIDOPENVOTE; }
		RaidOpenVote(void*) : Protocol(PROTOCOL_RAIDOPENVOTE) { }
		RaidOpenVote (int l_roleid,int l_voted_roleid = 0,int l_raidroom_id = 0,
			unsigned int l_localsid = 0)
			 : roleid(l_roleid),voted_roleid(l_voted_roleid),raidroom_id(l_raidroom_id)
			,localsid(l_localsid)
		{
			type = PROTOCOL_RAIDOPENVOTE;
		}

		RaidOpenVote(const RaidOpenVote &rhs)
			: Protocol(rhs),roleid(rhs.roleid),voted_roleid(rhs.voted_roleid),raidroom_id(rhs.raidroom_id)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new RaidOpenVote(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << voted_roleid;
			os << raidroom_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> voted_roleid;
			os >> raidroom_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
