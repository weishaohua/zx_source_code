		GNET::Protocol *Clone() const {  return new DBSNSUpdateMessage(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSNSUPDATEMESSAGE };
		DBSNSUpdateMessage(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSNSUpdateMessage(const DBSNSUpdateMessage &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 102400; }
