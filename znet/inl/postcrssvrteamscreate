		GNET::Protocol *Clone() const {  return new PostCrssvrTeamsCreate(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_POSTCRSSVRTEAMSCREATE };
		PostCrssvrTeamsCreate(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		PostCrssvrTeamsCreate(const PostCrssvrTeamsCreate &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1024; }
		bool TimePolicy(int timeout) const { return timeout <= 15; }
