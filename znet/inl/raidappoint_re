	public:
		int retcode;
		int roleid;
		int raidroom_id;
		int target;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_RAIDAPPOINT_RE };
	public:
		RaidAppoint_Re() { type = PROTOCOL_RAIDAPPOINT_RE; }
		RaidAppoint_Re(void*) : Protocol(PROTOCOL_RAIDAPPOINT_RE) { }
		RaidAppoint_Re (int l_retcode,int l_roleid = 0,int l_raidroom_id = 0,
			int l_target = 0,unsigned int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),raidroom_id(l_raidroom_id)
			,target(l_target),localsid(l_localsid)
		{
			type = PROTOCOL_RAIDAPPOINT_RE;
		}

		RaidAppoint_Re(const RaidAppoint_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),raidroom_id(rhs.raidroom_id)
			,target(rhs.target),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new RaidAppoint_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << raidroom_id;
			os << target;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> raidroom_id;
			os >> target;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
