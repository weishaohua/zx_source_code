	public:
		int retcode;
		int64_t sn;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_CONSIGNCANCELPOST_RE };
	public:
		ConsignCancelPost_Re() { type = PROTOCOL_CONSIGNCANCELPOST_RE; }
		ConsignCancelPost_Re(void*) : Protocol(PROTOCOL_CONSIGNCANCELPOST_RE) { }
		ConsignCancelPost_Re (int l_retcode,int64_t l_sn,unsigned int l_localsid)
			 : retcode(l_retcode),sn(l_sn),localsid(l_localsid)
		{
			type = PROTOCOL_CONSIGNCANCELPOST_RE;
		}

		ConsignCancelPost_Re(const ConsignCancelPost_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),sn(rhs.sn),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new ConsignCancelPost_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << sn;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> sn;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
