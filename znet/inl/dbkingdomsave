		GNET::Protocol *Clone() const {  return new DBKingdomSave(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBKINGDOMSAVE };
		DBKingdomSave(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBKingdomSave(const DBKingdomSave &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 131072; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
