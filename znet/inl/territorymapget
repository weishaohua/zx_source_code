	public:
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_TERRITORYMAPGET };
	public:
		TerritoryMapGet() { type = PROTOCOL_TERRITORYMAPGET; }
		TerritoryMapGet(void*) : Protocol(PROTOCOL_TERRITORYMAPGET) { }
		TerritoryMapGet (int l_roleid,unsigned int l_localsid = 0)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_TERRITORYMAPGET;
		}

		TerritoryMapGet(const TerritoryMapGet &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new TerritoryMapGet(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
