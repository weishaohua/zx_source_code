		GNET::Protocol *Clone() const {  return new SyncCrossCrssvrTeamsTop(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_SYNCCROSSCRSSVRTEAMSTOP };
		SyncCrossCrssvrTeamsTop(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		SyncCrossCrssvrTeamsTop(const SyncCrossCrssvrTeamsTop &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1024576; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
