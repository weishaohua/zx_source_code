	public:
		int retcode;
		unsigned int factionid;
		unsigned int localsid;
		char scale;
		char oper;
		enum { PROTOCOL_TYPE = PROTOCOL_FACTIONDISMISS_RE };
	public:
		FactionDismiss_Re() { type = PROTOCOL_FACTIONDISMISS_RE; }
		FactionDismiss_Re(void*) : Protocol(PROTOCOL_FACTIONDISMISS_RE) { }
		FactionDismiss_Re (int l_retcode,unsigned int l_factionid,unsigned int l_localsid,
			char l_scale = 0,char l_oper = 0)
			 : retcode(l_retcode),factionid(l_factionid),localsid(l_localsid)
			,scale(l_scale),oper(l_oper)
		{
			type = PROTOCOL_FACTIONDISMISS_RE;
		}

		FactionDismiss_Re(const FactionDismiss_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),factionid(rhs.factionid),localsid(rhs.localsid)
			,scale(rhs.scale),oper(rhs.oper) { }

		GNET::Protocol *Clone() const { return new FactionDismiss_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << factionid;
			os << localsid;
			os << scale;
			os << oper;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> factionid;
			os >> localsid;
			os >> scale;
			os >> oper;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
