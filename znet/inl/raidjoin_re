	public:
		int retcode;
		int raidroom_id;
		int map_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_RAIDJOIN_RE };
	public:
		RaidJoin_Re() { type = PROTOCOL_RAIDJOIN_RE; }
		RaidJoin_Re(void*) : Protocol(PROTOCOL_RAIDJOIN_RE) { }
		RaidJoin_Re (int l_retcode,int l_raidroom_id = 0,int l_map_id = 0,
			unsigned int l_localsid = 0)
			 : retcode(l_retcode),raidroom_id(l_raidroom_id),map_id(l_map_id)
			,localsid(l_localsid)
		{
			type = PROTOCOL_RAIDJOIN_RE;
		}

		RaidJoin_Re(const RaidJoin_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),raidroom_id(rhs.raidroom_id),map_id(rhs.map_id)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new RaidJoin_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << raidroom_id;
			os << map_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> raidroom_id;
			os >> map_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
