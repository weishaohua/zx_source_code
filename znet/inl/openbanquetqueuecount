	public:
		int roleid;
		unsigned int localsid;
		char iscross;
		enum { PROTOCOL_TYPE = PROTOCOL_OPENBANQUETQUEUECOUNT };
	public:
		OpenBanquetQueueCount() { type = PROTOCOL_OPENBANQUETQUEUECOUNT; }
		OpenBanquetQueueCount(void*) : Protocol(PROTOCOL_OPENBANQUETQUEUECOUNT) { }
		OpenBanquetQueueCount (int l_roleid,unsigned int l_localsid = 0,char l_iscross = 0)
			 : roleid(l_roleid),localsid(l_localsid),iscross(l_iscross)
		{
			type = PROTOCOL_OPENBANQUETQUEUECOUNT;
		}

		OpenBanquetQueueCount(const OpenBanquetQueueCount &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid),iscross(rhs.iscross) { }

		GNET::Protocol *Clone() const { return new OpenBanquetQueueCount(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			os << iscross;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			os >> iscross;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 20; }
