		GNET::Protocol *Clone() const {  return new DBSNSUpdateRoleInfo(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSNSUPDATEROLEINFO };
		DBSNSUpdateRoleInfo(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSNSUpdateRoleInfo(const DBSNSUpdateRoleInfo &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1024; }
