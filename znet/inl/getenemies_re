	public:
		char fulllist;
		GEnemyInfoVector enemies;
		RoleLinePairVector online;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_GETENEMIES_RE };
	public:
		GetEnemies_Re() { type = PROTOCOL_GETENEMIES_RE; }
		GetEnemies_Re(void*) : Protocol(PROTOCOL_GETENEMIES_RE) { }
		GetEnemies_Re (char l_fulllist,GEnemyInfoVector l_enemies,RoleLinePairVector l_online,
			unsigned int l_localsid)
			 : fulllist(l_fulllist),enemies(l_enemies),online(l_online)
			,localsid(l_localsid)
		{
			type = PROTOCOL_GETENEMIES_RE;
		}

		GetEnemies_Re(const GetEnemies_Re &rhs)
			: Protocol(rhs),fulllist(rhs.fulllist),enemies(rhs.enemies),online(rhs.online)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new GetEnemies_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << fulllist;
			os << enemies;
			os << online;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> fulllist;
			os >> enemies;
			os >> online;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 4096; }
