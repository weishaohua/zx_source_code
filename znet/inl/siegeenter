	public:
		int roleid;
		int battle_id;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_SIEGEENTER };
	public:
		SiegeEnter() { type = PROTOCOL_SIEGEENTER; }
		SiegeEnter(void*) : Protocol(PROTOCOL_SIEGEENTER) { }
		SiegeEnter (int l_roleid,int l_battle_id = 0,int l_localsid = 0)
			 : roleid(l_roleid),battle_id(l_battle_id),localsid(l_localsid)
		{
			type = PROTOCOL_SIEGEENTER;
		}

		SiegeEnter(const SiegeEnter &rhs)
			: Protocol(rhs),roleid(rhs.roleid),battle_id(rhs.battle_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new SiegeEnter(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << battle_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> battle_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
