		GNET::Protocol *Clone() const {  return new DBGateActivate(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBGATEACTIVATE };
		DBGateActivate(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBGateActivate(const DBGateActivate &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 32; }
		bool TimePolicy(int timeout) const { return timeout <= 60; }
