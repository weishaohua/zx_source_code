		GNET::Protocol *Clone() const {  return new DBSNSGetRole(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSNSGETROLE };
		DBSNSGetRole(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSNSGetRole(const DBSNSGetRole &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1048576; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
