		GNET::Protocol *Clone() const {  return new GUniqueBid(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GUNIQUEBID };
		GUniqueBid(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GUniqueBid(const GUniqueBid &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 64; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
