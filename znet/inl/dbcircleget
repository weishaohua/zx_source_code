		GNET::Protocol *Clone() const {  return new DBCircleGet(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCIRCLEGET };
		DBCircleGet(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBCircleGet(const DBCircleGet &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 524288; }
		bool TimePolicy(int timeout) const { return timeout <= 60; }
