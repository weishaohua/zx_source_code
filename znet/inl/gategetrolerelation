		GNET::Protocol *Clone() const {  return new GateGetRoleRelation(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GATEGETROLERELATION };
		GateGetRoleRelation(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GateGetRoleRelation(const GateGetRoleRelation &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 32768; }
		bool TimePolicy(int timeout) const { return timeout <= 60; }
