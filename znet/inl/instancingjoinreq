	public:
		int roleid;
		int map_id;
		int requester;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGJOINREQ };
	public:
		InstancingJoinReq() { type = PROTOCOL_INSTANCINGJOINREQ; }
		InstancingJoinReq(void*) : Protocol(PROTOCOL_INSTANCINGJOINREQ) { }
		InstancingJoinReq (int l_roleid,int l_map_id = 0,int l_requester = 0,
			unsigned int l_localsid = 0)
			 : roleid(l_roleid),map_id(l_map_id),requester(l_requester)
			,localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGJOINREQ;
		}

		InstancingJoinReq(const InstancingJoinReq &rhs)
			: Protocol(rhs),roleid(rhs.roleid),map_id(rhs.map_id),requester(rhs.requester)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingJoinReq(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << map_id;
			os << requester;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> map_id;
			os >> requester;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
