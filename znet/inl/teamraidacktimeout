	public:
		int raidroom_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_TEAMRAIDACKTIMEOUT };
	public:
		TeamRaidAckTimeout() { type = PROTOCOL_TEAMRAIDACKTIMEOUT; }
		TeamRaidAckTimeout(void*) : Protocol(PROTOCOL_TEAMRAIDACKTIMEOUT) { }
		TeamRaidAckTimeout (int l_raidroom_id,unsigned int l_localsid = 0)
			 : raidroom_id(l_raidroom_id),localsid(l_localsid)
		{
			type = PROTOCOL_TEAMRAIDACKTIMEOUT;
		}

		TeamRaidAckTimeout(const TeamRaidAckTimeout &rhs)
			: Protocol(rhs),raidroom_id(rhs.raidroom_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new TeamRaidAckTimeout(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << raidroom_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> raidroom_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
