	public:
		unsigned short retcode;
		int roleid;
		int map_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGSTART_RE };
	public:
		InstancingStart_Re() { type = PROTOCOL_INSTANCINGSTART_RE; }
		InstancingStart_Re(void*) : Protocol(PROTOCOL_INSTANCINGSTART_RE) { }
		InstancingStart_Re (unsigned short l_retcode,int l_roleid = 0,int l_map_id = 0,
			unsigned int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),map_id(l_map_id)
			,localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGSTART_RE;
		}

		InstancingStart_Re(const InstancingStart_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),map_id(rhs.map_id)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingStart_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << map_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> map_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
