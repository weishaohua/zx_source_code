	public:
		unsigned short accept;
		int roleid;
		int map_id;
		int requester;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGACCEPT };
	public:
		InstancingAccept() { type = PROTOCOL_INSTANCINGACCEPT; }
		InstancingAccept(void*) : Protocol(PROTOCOL_INSTANCINGACCEPT) { }
		InstancingAccept (unsigned short l_accept,int l_roleid = 0,int l_map_id = 0,
			int l_requester = 0,unsigned int l_localsid = 0)
			 : accept(l_accept),roleid(l_roleid),map_id(l_map_id)
			,requester(l_requester),localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGACCEPT;
		}

		InstancingAccept(const InstancingAccept &rhs)
			: Protocol(rhs),accept(rhs.accept),roleid(rhs.roleid),map_id(rhs.map_id)
			,requester(rhs.requester),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingAccept(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << accept;
			os << roleid;
			os << map_id;
			os << requester;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> accept;
			os >> roleid;
			os >> map_id;
			os >> requester;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
