	public:
		int retcode;
		int roleid;
		int localsid;
		int64_t points;
		enum { PROTOCOL_TYPE = PROTOCOL_GETTOUCHPOINT_RE };
	public:
		GetTouchPoint_Re() { type = PROTOCOL_GETTOUCHPOINT_RE; }
		GetTouchPoint_Re(void*) : Protocol(PROTOCOL_GETTOUCHPOINT_RE) { }
		GetTouchPoint_Re (int l_retcode,int l_roleid,int l_localsid = 0,
			int64_t l_points = 0)
			 : retcode(l_retcode),roleid(l_roleid),localsid(l_localsid)
			,points(l_points)
		{
			type = PROTOCOL_GETTOUCHPOINT_RE;
		}

		GetTouchPoint_Re(const GetTouchPoint_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),localsid(rhs.localsid)
			,points(rhs.points) { }

		GNET::Protocol *Clone() const { return new GetTouchPoint_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << localsid;
			os << points;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> localsid;
			os >> points;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
