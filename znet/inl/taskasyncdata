		GNET::Protocol *Clone() const {  return new TaskAsyncData(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_TASKASYNCDATA };
		TaskAsyncData(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		TaskAsyncData(const TaskAsyncData &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 8192; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
