		GNET::Protocol *Clone() const {  return new DBSiegeEnd(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSIEGEEND };
		DBSiegeEnd(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSiegeEnd(const DBSiegeEnd &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 128; }
		bool TimePolicy(int timeout) const { return timeout <= 40; }
