	public:
		int roleid;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_REFWITHDRAWEXP };
	public:
		RefWithdrawExp() { type = PROTOCOL_REFWITHDRAWEXP; }
		RefWithdrawExp(void*) : Protocol(PROTOCOL_REFWITHDRAWEXP) { }
		RefWithdrawExp (int l_roleid,int l_localsid = 0)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_REFWITHDRAWEXP;
		}

		RefWithdrawExp(const RefWithdrawExp &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new RefWithdrawExp(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
