	public:
		int retcode;
		int roleid;
		int message_id;
		char message_type;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_SNSPRESSMESSAGE_RE };
	public:
		SNSPressMessage_Re() { type = PROTOCOL_SNSPRESSMESSAGE_RE; }
		SNSPressMessage_Re(void*) : Protocol(PROTOCOL_SNSPRESSMESSAGE_RE) { }
		SNSPressMessage_Re (int l_retcode,int l_roleid = 0,int l_message_id = 0,
			char l_message_type = 0,int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),message_id(l_message_id)
			,message_type(l_message_type),localsid(l_localsid)
		{
			type = PROTOCOL_SNSPRESSMESSAGE_RE;
		}

		SNSPressMessage_Re(const SNSPressMessage_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),message_id(rhs.message_id)
			,message_type(rhs.message_type),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new SNSPressMessage_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << message_id;
			os << message_type;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> message_id;
			os >> message_type;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
