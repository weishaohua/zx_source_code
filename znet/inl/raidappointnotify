	public:
		int new_master_id;
		int raidroom_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_RAIDAPPOINTNOTIFY };
	public:
		RaidAppointNotify() { type = PROTOCOL_RAIDAPPOINTNOTIFY; }
		RaidAppointNotify(void*) : Protocol(PROTOCOL_RAIDAPPOINTNOTIFY) { }
		RaidAppointNotify (int l_new_master_id,int l_raidroom_id = 0,unsigned int l_localsid = 0)
			 : new_master_id(l_new_master_id),raidroom_id(l_raidroom_id),localsid(l_localsid)
		{
			type = PROTOCOL_RAIDAPPOINTNOTIFY;
		}

		RaidAppointNotify(const RaidAppointNotify &rhs)
			: Protocol(rhs),new_master_id(rhs.new_master_id),raidroom_id(rhs.raidroom_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new RaidAppointNotify(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << new_master_id;
			os << raidroom_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> new_master_id;
			os >> raidroom_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
