	public:
		int roleid;
		int battle_tid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGGETLIST };
	public:
		InstancingGetList() { type = PROTOCOL_INSTANCINGGETLIST; }
		InstancingGetList(void*) : Protocol(PROTOCOL_INSTANCINGGETLIST) { }
		InstancingGetList (int l_roleid,int l_battle_tid,unsigned int l_localsid = 0)
			 : roleid(l_roleid),battle_tid(l_battle_tid),localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGGETLIST;
		}

		InstancingGetList(const InstancingGetList &rhs)
			: Protocol(rhs),roleid(rhs.roleid),battle_tid(rhs.battle_tid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingGetList(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << battle_tid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> battle_tid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
