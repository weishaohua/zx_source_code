		GNET::Protocol *Clone() const {  return new DBSNSGetMessage(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSNSGETMESSAGE };
		DBSNSGetMessage(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSNSGetMessage(const DBSNSGetMessage &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 409600; }
