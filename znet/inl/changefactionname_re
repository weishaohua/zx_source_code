	public:
		int retcode;
		int roleid;
		int fid;
		Octets newname;
		char scale;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_CHANGEFACTIONNAME_RE };
	public:
		ChangeFactionName_Re() { type = PROTOCOL_CHANGEFACTIONNAME_RE; }
		ChangeFactionName_Re(void*) : Protocol(PROTOCOL_CHANGEFACTIONNAME_RE) { }
		ChangeFactionName_Re (int l_retcode,int l_roleid,int l_fid,
			const Octets& l_newname,char l_scale = 0,int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),fid(l_fid)
			,newname(l_newname),scale(l_scale),localsid(l_localsid)
		{
			type = PROTOCOL_CHANGEFACTIONNAME_RE;
		}

		ChangeFactionName_Re(const ChangeFactionName_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),fid(rhs.fid)
			,newname(rhs.newname),scale(rhs.scale),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new ChangeFactionName_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << fid;
			os << newname;
			os << scale;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> fid;
			os >> newname;
			os >> scale;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
