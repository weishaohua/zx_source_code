	public:
		int result;
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_PLAYERRECONNECT_RE };
	public:
		PlayerReconnect_Re() { type = PROTOCOL_PLAYERRECONNECT_RE; }
		PlayerReconnect_Re(void*) : Protocol(PROTOCOL_PLAYERRECONNECT_RE) { }
		PlayerReconnect_Re (int l_result,int l_roleid)
			 : result(l_result),roleid(l_roleid)
		{
			type = PROTOCOL_PLAYERRECONNECT_RE;
		}

		PlayerReconnect_Re(const PlayerReconnect_Re &rhs)
			: Protocol(rhs),result(rhs.result),roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new PlayerReconnect_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << result;
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> result;
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 101; }

		bool SizePolicy(size_t size) const { return size <= 32; }
