	public:
		int roleid;
		int retcode;
		enum { PROTOCOL_TYPE = PROTOCOL_GCONSIGNENDROLE };
	public:
		GConsignEndRole() { type = PROTOCOL_GCONSIGNENDROLE; }
		GConsignEndRole(void*) : Protocol(PROTOCOL_GCONSIGNENDROLE) { }
		GConsignEndRole (int l_roleid,int l_retcode)
			 : roleid(l_roleid),retcode(l_retcode)
		{
			type = PROTOCOL_GCONSIGNENDROLE;
		}

		GConsignEndRole(const GConsignEndRole &rhs)
			: Protocol(rhs),roleid(rhs.roleid),retcode(rhs.retcode) { }

		GNET::Protocol *Clone() const { return new GConsignEndRole(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> retcode;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
