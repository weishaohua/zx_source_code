		GNET::Protocol *Clone() const {  return new DBCheckAward(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCHECKAWARD };
		DBCheckAward(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBCheckAward(const DBCheckAward &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 512; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
