		GNET::Protocol *Clone() const {  return new SetLinePlayerLimit(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_SETLINEPLAYERLIMIT };
		SetLinePlayerLimit(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		SetLinePlayerLimit(const SetLinePlayerLimit &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 16384; }
		bool TimePolicy(int timeout) const { return timeout <= 10; }
