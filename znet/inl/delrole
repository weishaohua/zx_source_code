		GNET::Protocol *Clone() const {  return new DelRole(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DELROLE };
		DelRole(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DelRole(const DelRole &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 512; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
