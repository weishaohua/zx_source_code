		GNET::Protocol *Clone() const {  return new CommonDataChange(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_COMMONDATACHANGE };
		CommonDataChange(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		CommonDataChange(const CommonDataChange &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 101; }
		bool SizePolicy(size_t size) const { return size <= 10240; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
