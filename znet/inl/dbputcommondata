		GNET::Protocol *Clone() const {  return new DBPutCommonData(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBPUTCOMMONDATA };
		DBPutCommonData(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBPutCommonData(const DBPutCommonData &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 524288; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
