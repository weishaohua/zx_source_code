	public:
		int retcode;
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_FACBASEENTER_RE };
	public:
		FacBaseEnter_Re() { type = PROTOCOL_FACBASEENTER_RE; }
		FacBaseEnter_Re(void*) : Protocol(PROTOCOL_FACBASEENTER_RE) { }
		FacBaseEnter_Re (int l_retcode,int l_roleid,unsigned int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_FACBASEENTER_RE;
		}

		FacBaseEnter_Re(const FacBaseEnter_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new FacBaseEnter_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
