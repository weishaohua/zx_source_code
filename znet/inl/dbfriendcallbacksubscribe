		GNET::Protocol *Clone() const {  return new DBFriendCallbackSubscribe(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBFRIENDCALLBACKSUBSCRIBE };
		DBFriendCallbackSubscribe(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBFriendCallbackSubscribe(const DBFriendCallbackSubscribe &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 32; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
