	public:
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_QUEENTRYOPENBATH };
	public:
		QueenTryOpenBath() { type = PROTOCOL_QUEENTRYOPENBATH; }
		QueenTryOpenBath(void*) : Protocol(PROTOCOL_QUEENTRYOPENBATH) { }
		QueenTryOpenBath (int l_roleid,unsigned int l_localsid)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_QUEENTRYOPENBATH;
		}

		QueenTryOpenBath(const QueenTryOpenBath &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new QueenTryOpenBath(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
