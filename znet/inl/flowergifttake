		GNET::Protocol *Clone() const {  return new FlowerGiftTake(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_FLOWERGIFTTAKE };
		FlowerGiftTake(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		FlowerGiftTake(const FlowerGiftTake &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 256; }
		bool TimePolicy(int timeout) const { return timeout <= 90; }
