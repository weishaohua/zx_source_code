		GNET::Protocol *Clone() const {  return new DBSiegeLoad(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSIEGELOAD };
		DBSiegeLoad(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSiegeLoad(const DBSiegeLoad &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 10240; }
		bool TimePolicy(int timeout) const { return timeout <= 40; }
