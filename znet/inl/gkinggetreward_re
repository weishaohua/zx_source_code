	public:
		int retcode;
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_GKINGGETREWARD_RE };
	public:
		GKingGetReward_Re() { type = PROTOCOL_GKINGGETREWARD_RE; }
		GKingGetReward_Re(void*) : Protocol(PROTOCOL_GKINGGETREWARD_RE) { }
		GKingGetReward_Re (int l_retcode,int l_roleid)
			 : retcode(l_retcode),roleid(l_roleid)
		{
			type = PROTOCOL_GKINGGETREWARD_RE;
		}

		GKingGetReward_Re(const GKingGetReward_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new GKingGetReward_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
