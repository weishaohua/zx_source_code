	public:
		unsigned short retcode;
		int roleid;
		int endtime;
		char battle_type;
		unsigned int localsid;
		char iscross;
		enum { PROTOCOL_TYPE = PROTOCOL_BATTLEENTER_RE };
	public:
		BattleEnter_Re() { type = PROTOCOL_BATTLEENTER_RE; }
		BattleEnter_Re(void*) : Protocol(PROTOCOL_BATTLEENTER_RE) { }
		BattleEnter_Re (unsigned short l_retcode,int l_roleid,int l_endtime = 0,
			char l_battle_type = 0,unsigned int l_localsid = 0,char l_iscross = 0)
			 : retcode(l_retcode),roleid(l_roleid),endtime(l_endtime)
			,battle_type(l_battle_type),localsid(l_localsid),iscross(l_iscross)
		{
			type = PROTOCOL_BATTLEENTER_RE;
		}

		BattleEnter_Re(const BattleEnter_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),endtime(rhs.endtime)
			,battle_type(rhs.battle_type),localsid(rhs.localsid),iscross(rhs.iscross) { }

		GNET::Protocol *Clone() const { return new BattleEnter_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << endtime;
			os << battle_type;
			os << localsid;
			os << iscross;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> endtime;
			os >> battle_type;
			os >> localsid;
			os >> iscross;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
