		GNET::Protocol *Clone() const {  return new DBCircleHisGet(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCIRCLEHISGET };
		DBCircleHisGet(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBCircleHisGet(const DBCircleHisGet &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 524288; }
		bool TimePolicy(int timeout) const { return timeout <= 60; }
