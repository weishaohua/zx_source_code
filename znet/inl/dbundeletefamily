		GNET::Protocol *Clone() const {  return new DBUndeleteFamily(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBUNDELETEFAMILY };
		DBUndeleteFamily(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBUndeleteFamily(const DBUndeleteFamily &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 64; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
