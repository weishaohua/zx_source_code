		GNET::Protocol *Clone() const {  return new PostChangeFactionname(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_POSTCHANGEFACTIONNAME };
		PostChangeFactionname(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		PostChangeFactionname(const PostChangeFactionname &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 2097152; }
		bool TimePolicy(int timeout) const { return timeout <= 15; }
