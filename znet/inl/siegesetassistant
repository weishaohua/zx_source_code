	public:
		int roleid;
		int battle_id;
		Octets faction_name;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_SIEGESETASSISTANT };
	public:
		SiegeSetAssistant() { type = PROTOCOL_SIEGESETASSISTANT; }
		SiegeSetAssistant(void*) : Protocol(PROTOCOL_SIEGESETASSISTANT) { }
		SiegeSetAssistant (int l_roleid,int l_battle_id,const Octets& l_faction_name = Octets(0),
			unsigned int l_localsid = 0)
			 : roleid(l_roleid),battle_id(l_battle_id),faction_name(l_faction_name)
			,localsid(l_localsid)
		{
			type = PROTOCOL_SIEGESETASSISTANT;
		}

		SiegeSetAssistant(const SiegeSetAssistant &rhs)
			: Protocol(rhs),roleid(rhs.roleid),battle_id(rhs.battle_id),faction_name(rhs.faction_name)
			,localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new SiegeSetAssistant(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << battle_id;
			os << faction_name;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> battle_id;
			os >> faction_name;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
