	public:
		int retcode;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_UNIQUEGETITEM_RE };
	public:
		UniqueGetItem_Re() { type = PROTOCOL_UNIQUEGETITEM_RE; }
		UniqueGetItem_Re(void*) : Protocol(PROTOCOL_UNIQUEGETITEM_RE) { }
		UniqueGetItem_Re (int l_retcode,unsigned int l_localsid = 0)
			 : retcode(l_retcode),localsid(l_localsid)
		{
			type = PROTOCOL_UNIQUEGETITEM_RE;
		}

		UniqueGetItem_Re(const UniqueGetItem_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new UniqueGetItem_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
