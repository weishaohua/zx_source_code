		GNET::Protocol *Clone() const {  return new DBSNSGetPlayerInfoCache(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSNSGETPLAYERINFOCACHE };
		DBSNSGetPlayerInfoCache(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSNSGetPlayerInfoCache(const DBSNSGetPlayerInfoCache &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 2048; }
