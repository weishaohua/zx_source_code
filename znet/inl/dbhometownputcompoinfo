		GNET::Protocol *Clone() const {  return new DBHometownPutCompoInfo(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBHOMETOWNPUTCOMPOINFO };
		DBHometownPutCompoInfo(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBHometownPutCompoInfo(const DBHometownPutCompoInfo &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 163840; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
