	public:
		int roleid;
		int gs_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_OPENBANQUETGETFIELD };
	public:
		OpenBanquetGetField() { type = PROTOCOL_OPENBANQUETGETFIELD; }
		OpenBanquetGetField(void*) : Protocol(PROTOCOL_OPENBANQUETGETFIELD) { }
		OpenBanquetGetField (int l_roleid,int l_gs_id,unsigned int l_localsid = 0)
			 : roleid(l_roleid),gs_id(l_gs_id),localsid(l_localsid)
		{
			type = PROTOCOL_OPENBANQUETGETFIELD;
		}

		OpenBanquetGetField(const OpenBanquetGetField &rhs)
			: Protocol(rhs),roleid(rhs.roleid),gs_id(rhs.gs_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new OpenBanquetGetField(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << gs_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> gs_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
