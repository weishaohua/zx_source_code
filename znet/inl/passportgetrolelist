		GNET::Protocol *Clone() const {  return new PassportGetRoleList(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_PASSPORTGETROLELIST };
		PassportGetRoleList(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		PassportGetRoleList(const PassportGetRoleList &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1024; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
