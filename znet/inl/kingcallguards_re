	public:
		int retcode;
		unsigned int localsid;
		unsigned int remain_times;
		enum { PROTOCOL_TYPE = PROTOCOL_KINGCALLGUARDS_RE };
	public:
		KingCallGuards_Re() { type = PROTOCOL_KINGCALLGUARDS_RE; }
		KingCallGuards_Re(void*) : Protocol(PROTOCOL_KINGCALLGUARDS_RE) { }
		KingCallGuards_Re (int l_retcode,unsigned int l_localsid = 0,unsigned int l_remain_times = 0)
			 : retcode(l_retcode),localsid(l_localsid),remain_times(l_remain_times)
		{
			type = PROTOCOL_KINGCALLGUARDS_RE;
		}

		KingCallGuards_Re(const KingCallGuards_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),localsid(rhs.localsid),remain_times(rhs.remain_times) { }

		GNET::Protocol *Clone() const { return new KingCallGuards_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << localsid;
			os << remain_times;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> localsid;
			os >> remain_times;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
