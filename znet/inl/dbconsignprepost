		GNET::Protocol *Clone() const {  return new DBConsignPrePost(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCONSIGNPREPOST };
		DBConsignPrePost(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBConsignPrePost(const DBConsignPrePost &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 10485760; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
