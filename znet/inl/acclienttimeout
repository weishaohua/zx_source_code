	public:
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_ACCLIENTTIMEOUT };
	public:
		ACClientTimeout() { type = PROTOCOL_ACCLIENTTIMEOUT; }
		ACClientTimeout(void*) : Protocol(PROTOCOL_ACCLIENTTIMEOUT) { }
		ACClientTimeout (int l_roleid)
			 : roleid(l_roleid)
		{
			type = PROTOCOL_ACCLIENTTIMEOUT;
		}

		ACClientTimeout(const ACClientTimeout &rhs)
			: Protocol(rhs),roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new ACClientTimeout(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
