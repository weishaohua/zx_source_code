	public:
		int roleid;
		int gs_id;
		int map_id;
		unsigned int localsid;
		char iscross;
		enum { PROTOCOL_TYPE = PROTOCOL_BATTLEGETFIELD };
	public:
		BattleGetField() { type = PROTOCOL_BATTLEGETFIELD; }
		BattleGetField(void*) : Protocol(PROTOCOL_BATTLEGETFIELD) { }
		BattleGetField (int l_roleid,int l_gs_id,int l_map_id,
			unsigned int l_localsid = 0,char l_iscross = 0)
			 : roleid(l_roleid),gs_id(l_gs_id),map_id(l_map_id)
			,localsid(l_localsid),iscross(l_iscross)
		{
			type = PROTOCOL_BATTLEGETFIELD;
		}

		BattleGetField(const BattleGetField &rhs)
			: Protocol(rhs),roleid(rhs.roleid),gs_id(rhs.gs_id),map_id(rhs.map_id)
			,localsid(rhs.localsid),iscross(rhs.iscross) { }

		GNET::Protocol *Clone() const { return new BattleGetField(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << gs_id;
			os << map_id;
			os << localsid;
			os << iscross;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> gs_id;
			os >> map_id;
			os >> localsid;
			os >> iscross;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
