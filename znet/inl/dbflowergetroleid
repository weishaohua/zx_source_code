		GNET::Protocol *Clone() const {  return new DBFlowerGetRoleId(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBFLOWERGETROLEID };
		DBFlowerGetRoleId(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBFlowerGetRoleId(const DBFlowerGetRoleId &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 256; }
		bool TimePolicy(int timeout) const { return timeout <= 90; }
