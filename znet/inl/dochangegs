	public:
		int roleid;
		int dst_lineid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_DOCHANGEGS };
	public:
		DoChangeGS() { type = PROTOCOL_DOCHANGEGS; }
		DoChangeGS(void*) : Protocol(PROTOCOL_DOCHANGEGS) { }
		DoChangeGS (int l_roleid,int l_dst_lineid = 0,unsigned int l_localsid = 0)
			 : roleid(l_roleid),dst_lineid(l_dst_lineid),localsid(l_localsid)
		{
			type = PROTOCOL_DOCHANGEGS;
		}

		DoChangeGS(const DoChangeGS &rhs)
			: Protocol(rhs),roleid(rhs.roleid),dst_lineid(rhs.dst_lineid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new DoChangeGS(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << dst_lineid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> dst_lineid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
