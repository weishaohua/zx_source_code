	public:
		int roleid;
		char oper;
		char message_type;
		int message_id;
		Octets message;
		short dst_index;
		enum { PROTOCOL_TYPE = PROTOCOL_SENDSNSAPPLY };
	public:
		SendSNSApply() { type = PROTOCOL_SENDSNSAPPLY; }
		SendSNSApply(void*) : Protocol(PROTOCOL_SENDSNSAPPLY) { }
		SendSNSApply (int l_roleid,char l_oper = 0,char l_message_type = 0,
			int l_message_id = 0,const Octets& l_message = Octets(),short l_dst_index = 0)
			 : roleid(l_roleid),oper(l_oper),message_type(l_message_type)
			,message_id(l_message_id),message(l_message),dst_index(l_dst_index)
		{
			type = PROTOCOL_SENDSNSAPPLY;
		}

		SendSNSApply(const SendSNSApply &rhs)
			: Protocol(rhs),roleid(rhs.roleid),oper(rhs.oper),message_type(rhs.message_type)
			,message_id(rhs.message_id),message(rhs.message),dst_index(rhs.dst_index) { }

		GNET::Protocol *Clone() const { return new SendSNSApply(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << oper;
			os << message_type;
			os << message_id;
			os << message;
			os << dst_index;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> oper;
			os >> message_type;
			os >> message_id;
			os >> message;
			os >> dst_index;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 256; }
