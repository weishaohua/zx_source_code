		GNET::Protocol *Clone() const {  return new DBBattleJoin(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBBATTLEJOIN };
		DBBattleJoin(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBBattleJoin(const DBBattleJoin &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 2097152; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
