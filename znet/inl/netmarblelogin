		GNET::Protocol *Clone() const {  return new NetMarbleLogin(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_NETMARBLELOGIN };
		NetMarbleLogin(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		NetMarbleLogin(const NetMarbleLogin &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 104; }
		bool SizePolicy(size_t size) const { return size <= 256; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
