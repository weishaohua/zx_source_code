	public:
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_CONTESTEXIT };
	public:
		ContestExit() { type = PROTOCOL_CONTESTEXIT; }
		ContestExit(void*) : Protocol(PROTOCOL_CONTESTEXIT) { }
		ContestExit (int l_roleid)
			 : roleid(l_roleid)
		{
			type = PROTOCOL_CONTESTEXIT;
		}

		ContestExit(const ContestExit &rhs)
			: Protocol(rhs),roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new ContestExit(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
