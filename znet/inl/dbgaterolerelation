		GNET::Protocol *Clone() const {  return new DBGateRoleRelation(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBGATEROLERELATION };
		DBGateRoleRelation(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBGateRoleRelation(const DBGateRoleRelation &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 32768; }
		bool TimePolicy(int timeout) const { return timeout <= 60; }
