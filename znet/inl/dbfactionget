		GNET::Protocol *Clone() const {  return new DBFactionGet(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBFACTIONGET };
		DBFactionGet(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBFactionGet(const DBFactionGet &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 131072; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
