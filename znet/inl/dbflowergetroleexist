		GNET::Protocol *Clone() const {  return new DBFlowerGetRoleExist(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBFLOWERGETROLEEXIST };
		DBFlowerGetRoleExist(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBFlowerGetRoleExist(const DBFlowerGetRoleExist &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 256; }
		bool TimePolicy(int timeout) const { return timeout <= 90; }
