		GNET::Protocol *Clone() const {  return new DBConsignPrePostRole(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCONSIGNPREPOSTROLE };
		DBConsignPrePostRole(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBConsignPrePostRole(const DBConsignPrePostRole &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1048576; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
