	public:
		int retcode;
		unsigned short accept;
		int roleid;
		int map_id;
		int requester;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGACCEPT_RE };
	public:
		InstancingAccept_Re() { type = PROTOCOL_INSTANCINGACCEPT_RE; }
		InstancingAccept_Re(void*) : Protocol(PROTOCOL_INSTANCINGACCEPT_RE) { }
		InstancingAccept_Re (int l_retcode,unsigned short l_accept,int l_roleid = 0,
			int l_map_id = 0,int l_requester = 0,unsigned int l_localsid = 0)
			 : retcode(l_retcode),accept(l_accept),roleid(l_roleid)
			,map_id(l_map_id),requester(l_requester),localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGACCEPT_RE;
		}

		InstancingAccept_Re(const InstancingAccept_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),accept(rhs.accept),roleid(rhs.roleid)
			,map_id(rhs.map_id),requester(rhs.requester),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingAccept_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << accept;
			os << roleid;
			os << map_id;
			os << requester;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> accept;
			os >> roleid;
			os >> map_id;
			os >> requester;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
