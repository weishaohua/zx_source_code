	public:
		int retcode;
		int roleid;
		int gs_id;
		int map_id;
		char battle_type;
		char iscross;
		unsigned char team;
		int starttime;
		unsigned int localsid;
		int cooltime;
		enum { PROTOCOL_TYPE = PROTOCOL_BATTLEJOIN_RE };
	public:
		BattleJoin_Re() { type = PROTOCOL_BATTLEJOIN_RE; }
		BattleJoin_Re(void*) : Protocol(PROTOCOL_BATTLEJOIN_RE) { }
		BattleJoin_Re (int l_retcode,int l_roleid,int l_gs_id = 0,
			int l_map_id = 0,char l_battle_type = 0,char l_iscross = 0,
			unsigned char l_team = 0,int l_starttime = 0,unsigned int l_localsid = 0,
			int l_cooltime = -1)
			 : retcode(l_retcode),roleid(l_roleid),gs_id(l_gs_id)
			,map_id(l_map_id),battle_type(l_battle_type),iscross(l_iscross)
			,team(l_team),starttime(l_starttime),localsid(l_localsid)
			,cooltime(l_cooltime)
		{
			type = PROTOCOL_BATTLEJOIN_RE;
		}

		BattleJoin_Re(const BattleJoin_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),gs_id(rhs.gs_id)
			,map_id(rhs.map_id),battle_type(rhs.battle_type),iscross(rhs.iscross)
			,team(rhs.team),starttime(rhs.starttime),localsid(rhs.localsid)
			,cooltime(rhs.cooltime) { }

		GNET::Protocol *Clone() const { return new BattleJoin_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << gs_id;
			os << map_id;
			os << battle_type;
			os << iscross;
			os << team;
			os << starttime;
			os << localsid;
			os << cooltime;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> gs_id;
			os >> map_id;
			os >> battle_type;
			os >> iscross;
			os >> team;
			os >> starttime;
			os >> localsid;
			os >> cooltime;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
