	public:
		unsigned short retcode;
		int endtime;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGENTER_RE };
	public:
		InstancingEnter_Re() { type = PROTOCOL_INSTANCINGENTER_RE; }
		InstancingEnter_Re(void*) : Protocol(PROTOCOL_INSTANCINGENTER_RE) { }
		InstancingEnter_Re (unsigned short l_retcode,int l_endtime = 0,unsigned int l_localsid = 0)
			 : retcode(l_retcode),endtime(l_endtime),localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGENTER_RE;
		}

		InstancingEnter_Re(const InstancingEnter_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),endtime(rhs.endtime),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingEnter_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << endtime;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> endtime;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
