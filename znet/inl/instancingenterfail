	public:
		int roleid;
		int map_id;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGENTERFAIL };
	public:
		InstancingEnterFail() { type = PROTOCOL_INSTANCINGENTERFAIL; }
		InstancingEnterFail(void*) : Protocol(PROTOCOL_INSTANCINGENTERFAIL) { }
		InstancingEnterFail (int l_roleid,int l_map_id = 0)
			 : roleid(l_roleid),map_id(l_map_id)
		{
			type = PROTOCOL_INSTANCINGENTERFAIL;
		}

		InstancingEnterFail(const InstancingEnterFail &rhs)
			: Protocol(rhs),roleid(rhs.roleid),map_id(rhs.map_id) { }

		GNET::Protocol *Clone() const { return new InstancingEnterFail(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << map_id;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> map_id;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
