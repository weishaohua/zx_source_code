	public:
		char fieldtype;
		int tagid;
		enum { PROTOCOL_TYPE = PROTOCOL_KINGDOMBATTLESTOP };
	public:
		KingdomBattleStop() { type = PROTOCOL_KINGDOMBATTLESTOP; }
		KingdomBattleStop(void*) : Protocol(PROTOCOL_KINGDOMBATTLESTOP) { }
		KingdomBattleStop (char l_fieldtype,int l_tagid = 0)
			 : fieldtype(l_fieldtype),tagid(l_tagid)
		{
			type = PROTOCOL_KINGDOMBATTLESTOP;
		}

		KingdomBattleStop(const KingdomBattleStop &rhs)
			: Protocol(rhs),fieldtype(rhs.fieldtype),tagid(rhs.tagid) { }

		GNET::Protocol *Clone() const { return new KingdomBattleStop(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << fieldtype;
			os << tagid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> fieldtype;
			os >> tagid;
			return os;
		}

		bool SizePolicy(size_t size) const { return size <= 128; }
