	public:
		int retcode;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_TOPFLOWER_ERR };
	public:
		TopFlower_Err() { type = PROTOCOL_TOPFLOWER_ERR; }
		TopFlower_Err(void*) : Protocol(PROTOCOL_TOPFLOWER_ERR) { }
		TopFlower_Err (int l_retcode,unsigned int l_localsid = 0)
			 : retcode(l_retcode),localsid(l_localsid)
		{
			type = PROTOCOL_TOPFLOWER_ERR;
		}

		TopFlower_Err(const TopFlower_Err &rhs)
			: Protocol(rhs),retcode(rhs.retcode),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new TopFlower_Err(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
