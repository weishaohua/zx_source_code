		GNET::Protocol *Clone() const {  return new PutRoleBase2(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_PUTROLEBASE2 };
		PutRoleBase2(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		PutRoleBase2(const PutRoleBase2 &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1048576; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
