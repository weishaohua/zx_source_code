	public:
		int retcode;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_KINGDOMBATTLELEAVE_RE };
	public:
		KingdomBattleLeave_Re() { type = PROTOCOL_KINGDOMBATTLELEAVE_RE; }
		KingdomBattleLeave_Re(void*) : Protocol(PROTOCOL_KINGDOMBATTLELEAVE_RE) { }
		KingdomBattleLeave_Re (int l_retcode,int l_localsid = 0)
			 : retcode(l_retcode),localsid(l_localsid)
		{
			type = PROTOCOL_KINGDOMBATTLELEAVE_RE;
		}

		KingdomBattleLeave_Re(const KingdomBattleLeave_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new KingdomBattleLeave_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
