		GNET::Protocol *Clone() const {  return new DBConsignTableLoad(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBCONSIGNTABLELOAD };
		DBConsignTableLoad(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBConsignTableLoad(const DBConsignTableLoad &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 262144; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
