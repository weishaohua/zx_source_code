	public:
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_KINGDOMGETINFO };
	public:
		KingdomGetInfo() { type = PROTOCOL_KINGDOMGETINFO; }
		KingdomGetInfo(void*) : Protocol(PROTOCOL_KINGDOMGETINFO) { }
		KingdomGetInfo (int l_roleid,unsigned int l_localsid)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_KINGDOMGETINFO;
		}

		KingdomGetInfo(const KingdomGetInfo &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new KingdomGetInfo(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
