	public:
		int userid;
		int zoneid;
		enum { PROTOCOL_TYPE = PROTOCOL_QUERYUSERFORBID2 };
	public:
		QueryUserForbid2() { type = PROTOCOL_QUERYUSERFORBID2; }
		QueryUserForbid2(void*) : Protocol(PROTOCOL_QUERYUSERFORBID2) { }
		QueryUserForbid2 (int l_userid,int l_zoneid)
			 : userid(l_userid),zoneid(l_zoneid)
		{
			type = PROTOCOL_QUERYUSERFORBID2;
		}

		QueryUserForbid2(const QueryUserForbid2 &rhs)
			: Protocol(rhs),userid(rhs.userid),zoneid(rhs.zoneid) { }

		GNET::Protocol *Clone() const { return new QueryUserForbid2(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << userid;
			os << zoneid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> userid;
			os >> zoneid;
			return os;
		}

		int PriorPolicy( ) const { return 2; }

		bool SizePolicy(size_t size) const { return size <= 32; }
