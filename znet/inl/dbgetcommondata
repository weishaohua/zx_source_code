		GNET::Protocol *Clone() const {  return new DBGetCommonData(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBGETCOMMONDATA };
		DBGetCommonData(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBGetCommonData(const DBGetCommonData &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 524288; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
