		GNET::Protocol *Clone() const {  return new DBGetMailAll(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBGETMAILALL };
		DBGetMailAll(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBGetMailAll(const DBGetMailAll &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 102400; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
