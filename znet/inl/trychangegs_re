	public:
		int retcode;
		int roleid;
		int dst_lineid;
		unsigned int localsid;
		int reason;
		enum { PROTOCOL_TYPE = PROTOCOL_TRYCHANGEGS_RE };
	public:
		TryChangeGS_Re() { type = PROTOCOL_TRYCHANGEGS_RE; }
		TryChangeGS_Re(void*) : Protocol(PROTOCOL_TRYCHANGEGS_RE) { }
		TryChangeGS_Re (int l_retcode,int l_roleid,int l_dst_lineid = 0,
			unsigned int l_localsid = 0,int l_reason = 0)
			 : retcode(l_retcode),roleid(l_roleid),dst_lineid(l_dst_lineid)
			,localsid(l_localsid),reason(l_reason)
		{
			type = PROTOCOL_TRYCHANGEGS_RE;
		}

		TryChangeGS_Re(const TryChangeGS_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),dst_lineid(rhs.dst_lineid)
			,localsid(rhs.localsid),reason(rhs.reason) { }

		GNET::Protocol *Clone() const { return new TryChangeGS_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << dst_lineid;
			os << localsid;
			os << reason;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> dst_lineid;
			os >> localsid;
			os >> reason;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
