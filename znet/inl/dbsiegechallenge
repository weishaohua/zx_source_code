		GNET::Protocol *Clone() const {  return new DBSiegeChallenge(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBSIEGECHALLENGE };
		DBSiegeChallenge(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBSiegeChallenge(const DBSiegeChallenge &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 2097152; }
		bool TimePolicy(int timeout) const { return timeout <= 40; }
