	public:
		int roleid;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_GETTOUCHPOINT };
	public:
		GetTouchPoint() { type = PROTOCOL_GETTOUCHPOINT; }
		GetTouchPoint(void*) : Protocol(PROTOCOL_GETTOUCHPOINT) { }
		GetTouchPoint (int l_roleid,int l_localsid = 0)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_GETTOUCHPOINT;
		}

		GetTouchPoint(const GetTouchPoint &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new GetTouchPoint(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
