	public:
		int roleid;
		unsigned int localsid;
		unsigned int linksid;
		int userid;
		enum { PROTOCOL_TYPE = PROTOCOL_GETKDCTOKEN };
	public:
		GetKDCToken() { type = PROTOCOL_GETKDCTOKEN; }
		GetKDCToken(void*) : Protocol(PROTOCOL_GETKDCTOKEN) { }
		GetKDCToken (int l_roleid,unsigned int l_localsid = 0,unsigned int l_linksid = 0,
			int l_userid = 0)
			 : roleid(l_roleid),localsid(l_localsid),linksid(l_linksid)
			,userid(l_userid)
		{
			type = PROTOCOL_GETKDCTOKEN;
		}

		GetKDCToken(const GetKDCToken &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid),linksid(rhs.linksid)
			,userid(rhs.userid) { }

		GNET::Protocol *Clone() const { return new GetKDCToken(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			os << linksid;
			os << userid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			os >> linksid;
			os >> userid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
