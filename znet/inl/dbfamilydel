		GNET::Protocol *Clone() const {  return new DBFamilyDel(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBFAMILYDEL };
		DBFamilyDel(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBFamilyDel(const DBFamilyDel &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 1024; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
