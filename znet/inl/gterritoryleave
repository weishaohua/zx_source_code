	public:
		int roleid;
		int territoryid;
		int map_tag;
		float scalex;
		float scaley;
		float scalez;
		enum { PROTOCOL_TYPE = PROTOCOL_GTERRITORYLEAVE };
	public:
		GTerritoryLeave() { type = PROTOCOL_GTERRITORYLEAVE; }
		GTerritoryLeave(void*) : Protocol(PROTOCOL_GTERRITORYLEAVE) { }
		GTerritoryLeave (int l_roleid,int l_territoryid = 0,int l_map_tag = -1,
			float l_scalex = 0.0f,float l_scaley = 0.0f,float l_scalez = 0.0f)
			 : roleid(l_roleid),territoryid(l_territoryid),map_tag(l_map_tag)
			,scalex(l_scalex),scaley(l_scaley),scalez(l_scalez)
		{
			type = PROTOCOL_GTERRITORYLEAVE;
		}

		GTerritoryLeave(const GTerritoryLeave &rhs)
			: Protocol(rhs),roleid(rhs.roleid),territoryid(rhs.territoryid),map_tag(rhs.map_tag)
			,scalex(rhs.scalex),scaley(rhs.scaley),scalez(rhs.scalez) { }

		GNET::Protocol *Clone() const { return new GTerritoryLeave(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << territoryid;
			os << map_tag;
			os << scalex;
			os << scaley;
			os << scalez;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> territoryid;
			os >> map_tag;
			os >> scalex;
			os >> scaley;
			os >> scalez;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
