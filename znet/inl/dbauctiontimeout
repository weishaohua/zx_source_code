		GNET::Protocol *Clone() const {  return new DBAuctionTimeout(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBAUCTIONTIMEOUT };
		DBAuctionTimeout(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBAuctionTimeout(const DBAuctionTimeout &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 2048; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
