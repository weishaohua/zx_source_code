	public:
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_QUERYNETBARREWARD };
	public:
		QueryNetBarReward() { type = PROTOCOL_QUERYNETBARREWARD; }
		QueryNetBarReward(void*) : Protocol(PROTOCOL_QUERYNETBARREWARD) { }
		QueryNetBarReward (int l_roleid)
			 : roleid(l_roleid)
		{
			type = PROTOCOL_QUERYNETBARREWARD;
		}

		QueryNetBarReward(const QueryNetBarReward &rhs)
			: Protocol(rhs),roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new QueryNetBarReward(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 101; }

		bool SizePolicy(size_t size) const { return size <= 16; }
