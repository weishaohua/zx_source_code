	public:
		int zoneid;
		int aid;
		char blreset;
		int ip1;
		int ip2;
		int ip3;
		int version;
		int reserved2;
		enum { PROTOCOL_TYPE = PROTOCOL_ANNOUNCEZONEID3 };
	public:
		AnnounceZoneid3() { type = PROTOCOL_ANNOUNCEZONEID3; }
		AnnounceZoneid3(void*) : Protocol(PROTOCOL_ANNOUNCEZONEID3) { }
		AnnounceZoneid3 (int l_zoneid,int l_aid,char l_blreset,
			int l_ip1,int l_ip2,int l_ip3,
			int l_version,int l_reserved2)
			 : zoneid(l_zoneid),aid(l_aid),blreset(l_blreset)
			,ip1(l_ip1),ip2(l_ip2),ip3(l_ip3)
			,version(l_version),reserved2(l_reserved2)
		{
			type = PROTOCOL_ANNOUNCEZONEID3;
		}

		AnnounceZoneid3(const AnnounceZoneid3 &rhs)
			: Protocol(rhs),zoneid(rhs.zoneid),aid(rhs.aid),blreset(rhs.blreset)
			,ip1(rhs.ip1),ip2(rhs.ip2),ip3(rhs.ip3)
			,version(rhs.version),reserved2(rhs.reserved2) { }

		GNET::Protocol *Clone() const { return new AnnounceZoneid3(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << zoneid;
			os << aid;
			os << blreset;
			os << ip1;
			os << ip2;
			os << ip3;
			os << version;
			os << reserved2;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> zoneid;
			os >> aid;
			os >> blreset;
			os >> ip1;
			os >> ip2;
			os >> ip3;
			os >> version;
			os >> reserved2;
			return os;
		}

		int PriorPolicy( ) const { return 2; }

		bool SizePolicy(size_t size) const { return size <= 64; }
