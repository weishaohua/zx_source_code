	public:
		int roleid;
		int gs_id;
		int map_id;
		unsigned char team;
		GMailSyncData syncdata;
		enum { PROTOCOL_TYPE = PROTOCOL_SENDINSTANCINGJOIN };
	public:
		SendInstancingJoin() { type = PROTOCOL_SENDINSTANCINGJOIN; }
		SendInstancingJoin(void*) : Protocol(PROTOCOL_SENDINSTANCINGJOIN) { }
		SendInstancingJoin (int l_roleid,int l_gs_id = 0,int l_map_id = 0,
			unsigned char l_team = 0,const GMailSyncData& l_syncdata = GMailSyncData())
			 : roleid(l_roleid),gs_id(l_gs_id),map_id(l_map_id)
			,team(l_team),syncdata(l_syncdata)
		{
			type = PROTOCOL_SENDINSTANCINGJOIN;
		}

		SendInstancingJoin(const SendInstancingJoin &rhs)
			: Protocol(rhs),roleid(rhs.roleid),gs_id(rhs.gs_id),map_id(rhs.map_id)
			,team(rhs.team),syncdata(rhs.syncdata) { }

		GNET::Protocol *Clone() const { return new SendInstancingJoin(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << gs_id;
			os << map_id;
			os << team;
			os << syncdata;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> gs_id;
			os >> map_id;
			os >> team;
			os >> syncdata;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 2097152; }
