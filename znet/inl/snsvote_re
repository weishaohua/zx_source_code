	public:
		int retcode;
		int roleid;
		char oper;
		int message_id;
		char message_type;
		int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_SNSVOTE_RE };
	public:
		SNSVote_Re() { type = PROTOCOL_SNSVOTE_RE; }
		SNSVote_Re(void*) : Protocol(PROTOCOL_SNSVOTE_RE) { }
		SNSVote_Re (int l_retcode,int l_roleid = 0,char l_oper = 0,
			int l_message_id = 0,char l_message_type = 0,int l_localsid = 0)
			 : retcode(l_retcode),roleid(l_roleid),oper(l_oper)
			,message_id(l_message_id),message_type(l_message_type),localsid(l_localsid)
		{
			type = PROTOCOL_SNSVOTE_RE;
		}

		SNSVote_Re(const SNSVote_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),oper(rhs.oper)
			,message_id(rhs.message_id),message_type(rhs.message_type),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new SNSVote_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << oper;
			os << message_id;
			os << message_type;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> oper;
			os >> message_id;
			os >> message_type;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 256; }
