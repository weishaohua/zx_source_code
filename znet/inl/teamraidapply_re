	public:
		int retcode;
		int err_roleid;
		unsigned int localsid;
		int roleid;
		enum { PROTOCOL_TYPE = PROTOCOL_TEAMRAIDAPPLY_RE };
	public:
		TeamRaidApply_Re() { type = PROTOCOL_TEAMRAIDAPPLY_RE; }
		TeamRaidApply_Re(void*) : Protocol(PROTOCOL_TEAMRAIDAPPLY_RE) { }
		TeamRaidApply_Re (int l_retcode,int l_err_roleid = 0,unsigned int l_localsid = 0,
			int l_roleid = 0)
			 : retcode(l_retcode),err_roleid(l_err_roleid),localsid(l_localsid)
			,roleid(l_roleid)
		{
			type = PROTOCOL_TEAMRAIDAPPLY_RE;
		}

		TeamRaidApply_Re(const TeamRaidApply_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),err_roleid(rhs.err_roleid),localsid(rhs.localsid)
			,roleid(rhs.roleid) { }

		GNET::Protocol *Clone() const { return new TeamRaidApply_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << err_roleid;
			os << localsid;
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> err_roleid;
			os >> localsid;
			os >> roleid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
