		GNET::Protocol *Clone() const {  return new ModifyRoleCreateTime(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_MODIFYROLECREATETIME };
		ModifyRoleCreateTime(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		ModifyRoleCreateTime(const ModifyRoleCreateTime &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 16; }
		bool TimePolicy(int timeout) const { return timeout <= 30; }
