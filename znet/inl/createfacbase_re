	public:
		int retcode;
		int roleid;
		unsigned int localsid;
		char enterable;
		enum { PROTOCOL_TYPE = PROTOCOL_CREATEFACBASE_RE };
	public:
		CreateFacBase_Re() { type = PROTOCOL_CREATEFACBASE_RE; }
		CreateFacBase_Re(void*) : Protocol(PROTOCOL_CREATEFACBASE_RE) { }
		CreateFacBase_Re (int l_retcode,int l_roleid,unsigned int l_localsid = 0,
			char l_enterable = 0)
			 : retcode(l_retcode),roleid(l_roleid),localsid(l_localsid)
			,enterable(l_enterable)
		{
			type = PROTOCOL_CREATEFACBASE_RE;
		}

		CreateFacBase_Re(const CreateFacBase_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),localsid(rhs.localsid)
			,enterable(rhs.enterable) { }

		GNET::Protocol *Clone() const { return new CreateFacBase_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << localsid;
			os << enterable;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> localsid;
			os >> enterable;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
