		GNET::Protocol *Clone() const {  return new TransactionCommit(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_TRANSACTIONCOMMIT };
		TransactionCommit(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		TransactionCommit(const TransactionCommit &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 128; }
		bool TimePolicy(int timeout) const { return timeout <= 10; }
