	public:
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_FRIENDCALLBACKAWARD };
	public:
		FriendCallbackAward() { type = PROTOCOL_FRIENDCALLBACKAWARD; }
		FriendCallbackAward(void*) : Protocol(PROTOCOL_FRIENDCALLBACKAWARD) { }
		FriendCallbackAward (int l_roleid,unsigned int l_localsid)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_FRIENDCALLBACKAWARD;
		}

		FriendCallbackAward(const FriendCallbackAward &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new FriendCallbackAward(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
