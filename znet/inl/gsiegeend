	public:
		int battle_id;
		int world_tag;
		int result;
		enum { PROTOCOL_TYPE = PROTOCOL_GSIEGEEND };
	public:
		GSiegeEnd() { type = PROTOCOL_GSIEGEEND; }
		GSiegeEnd(void*) : Protocol(PROTOCOL_GSIEGEEND) { }
		GSiegeEnd (int l_battle_id,int l_world_tag = 0,int l_result = 0)
			 : battle_id(l_battle_id),world_tag(l_world_tag),result(l_result)
		{
			type = PROTOCOL_GSIEGEEND;
		}

		GSiegeEnd(const GSiegeEnd &rhs)
			: Protocol(rhs),battle_id(rhs.battle_id),world_tag(rhs.world_tag),result(rhs.result) { }

		GNET::Protocol *Clone() const { return new GSiegeEnd(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << battle_id;
			os << world_tag;
			os << result;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> battle_id;
			os >> world_tag;
			os >> result;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
