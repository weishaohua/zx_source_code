	public:
		int retcode;
		int roleid;
		unsigned int localsid;
		char iscross;
		enum { PROTOCOL_TYPE = PROTOCOL_BATTLELEAVE_RE };
	public:
		BattleLeave_Re() { type = PROTOCOL_BATTLELEAVE_RE; }
		BattleLeave_Re(void*) : Protocol(PROTOCOL_BATTLELEAVE_RE) { }
		BattleLeave_Re (int l_retcode,int l_roleid,unsigned int l_localsid = 0,
			char l_iscross = 0)
			 : retcode(l_retcode),roleid(l_roleid),localsid(l_localsid)
			,iscross(l_iscross)
		{
			type = PROTOCOL_BATTLELEAVE_RE;
		}

		BattleLeave_Re(const BattleLeave_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),localsid(rhs.localsid)
			,iscross(rhs.iscross) { }

		GNET::Protocol *Clone() const { return new BattleLeave_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << localsid;
			os << iscross;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> localsid;
			os >> iscross;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
