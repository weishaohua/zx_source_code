		GNET::Protocol *Clone() const {  return new DBKingGetReward(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_DBKINGGETREWARD };
		DBKingGetReward(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		DBKingGetReward(const DBKingGetReward &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 128; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
