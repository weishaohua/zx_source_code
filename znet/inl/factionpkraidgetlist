	public:
		int roleid;
		int map_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_FACTIONPKRAIDGETLIST };
	public:
		FactionPkRaidGetList() { type = PROTOCOL_FACTIONPKRAIDGETLIST; }
		FactionPkRaidGetList(void*) : Protocol(PROTOCOL_FACTIONPKRAIDGETLIST) { }
		FactionPkRaidGetList (int l_roleid,int l_map_id = 0,unsigned int l_localsid = 0)
			 : roleid(l_roleid),map_id(l_map_id),localsid(l_localsid)
		{
			type = PROTOCOL_FACTIONPKRAIDGETLIST;
		}

		FactionPkRaidGetList(const FactionPkRaidGetList &rhs)
			: Protocol(rhs),roleid(rhs.roleid),map_id(rhs.map_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new FactionPkRaidGetList(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << map_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> map_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
