	public:
		int retcode;
		int map_id;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_INSTANCINGLEAVE_RE };
	public:
		InstancingLeave_Re() { type = PROTOCOL_INSTANCINGLEAVE_RE; }
		InstancingLeave_Re(void*) : Protocol(PROTOCOL_INSTANCINGLEAVE_RE) { }
		InstancingLeave_Re (int l_retcode,int l_map_id,unsigned int l_localsid = 0)
			 : retcode(l_retcode),map_id(l_map_id),localsid(l_localsid)
		{
			type = PROTOCOL_INSTANCINGLEAVE_RE;
		}

		InstancingLeave_Re(const InstancingLeave_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),map_id(rhs.map_id),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new InstancingLeave_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << map_id;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> map_id;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
